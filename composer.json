{
  "name": "commercie/payment",
  "description": "A framework for defining and processing payments.",
  "license": "MIT",
  "authors": [
    {
      "name": "Bart Feenstra",
      "email": "bart@mynameisbart.com"
    }
  ],
  "require": {
    "php": "~5.5 || ~7.0",
    "commercie/money": "0.1.*"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "suggest": {
    "commercie/payment-symfony-event-dispatcher-bridge": "Integrates this package with symfony/event-dispatcher.",
    "commercie/payment-symfony-http-foundation-bridge": "Integrates this package with symfony/http-foundation."
  },
  "autoload": {
    "psr-4": {
      "Commercie\\Payment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Commercie\\Tests\\Payment\\": "tests"
    }
  }
}
